{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install": "husky install",
    "copy-gql": "copyfiles -u 1 src/**/*.gql build/",
    "build": "yarn format && tsc --noEmit && tsup && yarn copy-gql",
    "build:pre-commit": "tsc --noEmit && tsup",
    "lint": "eslint \"**/*.{ts, tsx}\" --quiet --fix",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "set TEST=true && cross-env DOTENV_CONFIG_PATH=.env.test jest --setupFiles dotenv/config --verbose true",
    "test:cov": "set TEST=true && yarn test --coverage --forceExit --detectOpenHandles --verbose",
    "debug:project-services": "env-cmd --verbose nodemon --exec \"node --inspect-brk=8101 -r esbuild-register src/project-services/app.ts\" -e .ts & tsc -w --pretty --skipLibCheck --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/aws-lambda": "^4.1.0",
    "@fastify/cors": "^9.0.1",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.7",
    "bcrypt": "^5.1.1",
    "fastify": "^4.28.1",
    "graphql": "^16.9.0",
    "jsonwebtoken": "^9.0.2",
    "mercurius": "^14.1.0",
    "pg": "^8.12.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.36",
    "@types/aws-lambda": "^8.10.145",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.23.1",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
